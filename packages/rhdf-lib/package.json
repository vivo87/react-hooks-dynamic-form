{
  "name": "react-hooks-dynamic-form",
  "version": "0.1.0-beta.1",
  "description": "Dynamic form with react hooks",
  "keywords": [
    "react",
    "hooks",
    "form"
  ],
  "author": "vivo87 <vongocviet@gmail.com>",
  "homepage": "https://github.com/vivo87/react-hooks-dynamic-form",
  "license": "ISC",
  "main": "dist/index.js",
  "files": [
    "src/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vivo87/react-hooks-dynamic-form.git"
  },
  "scripts": {
    "start": "concurrently \"tsc --watch\"",
    "build": "tsc",
    "test": "jest",
    "test-commit": "jest --bail --findRelatedTests",
    "format": "prettier --config ../../.prettierrc --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --ext=jsx,ts,tsx src"
  },
  "bugs": {
    "url": "https://github.com/vivo87/react-hooks-dynamic-form/issues"
  },
  "devDependencies": {
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.8",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "concurrently": "^5.0.0",
    "jest": "^24.9.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/src/.*.(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "json", "node"]
  }
}
